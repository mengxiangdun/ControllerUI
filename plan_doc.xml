                        <Button id="" text="-X" repeat="0" rate="50" cmd="movePoint --cor=1  --x=-0.01" return_attached=""/>
                        <Button id="" text="+Y" repeat="0" rate="50" cmd="movePoint --cor=1  --y=0.01" return_attached=""/>
                        <Button id="" text="-Y" repeat="0" rate="50" cmd="movePoint --cor=1  --y=-0.01" return_attached=""/>
                        <Button id="" text="+Z" repeat="0" rate="50" cmd="movePoint --cor=1  --z=0.01" return_attached=""/>
                        <Button id="" text="-Z" repeat="0" rate="50" cmd="movePoint  --cor=1 --z=-0.01" return_attached=""/>
                        <Button id="" text="+RX" repeat="0" rate="50" cmd="movePoint  --cor=1 --a=0.01" return_attached=""/>
                        <Button id="" text="-RX" repeat="0" rate="50" cmd="movePoint  --cor=1 --a=-0.01" return_attached=""/>
